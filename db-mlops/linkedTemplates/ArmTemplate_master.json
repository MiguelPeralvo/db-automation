{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "db-mlops"
		},
		"db_mlops_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'db_mlops'"
		},
		"db_mlops_staging_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'db_mlops_staging'"
		},
		"AzureKeyVault_ADF_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://miguel-kv.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"db_mlops_accessToken": {
						"value": "[parameters('db_mlops_accessToken')]"
					},
					"db_mlops_staging_accessToken": {
						"value": "[parameters('db_mlops_staging_accessToken')]"
					},
					"AzureKeyVault_ADF_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_ADF_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}